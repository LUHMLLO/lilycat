{
    "author": "luhmllo",
    "description": "props powered css lib",
    "homepage": "https://lilys-kit.vercel.app/",
    "license": "MIT",
    "name": "lilys",
    "type": "module",
    "version": "0.0.18",
    "browserslist": [
        "defaults",
        "not op_mini all"
    ],
    "devDependencies": {
        "cssnano": "^7.0.2",
        "cssnano-preset-advanced": "^7.0.2",
        "postcss": "^8.4.38",
        "postcss-cli": "^11.0.0",
        "postcss-fail-on-warn": "^0.2.1",
        "postcss-import": "^16.1.0",
        "sass": "^1.77.5"
    },
    "scripts": {
        "gen:scss": "sass --color --no-source-map --stop-on-error --unicode --update ./src/sass/:./src/css/",
        "gen:postcss": "postcss ./src/css/*.css -d ./build/ --config ./.postcssrc"
    }
}