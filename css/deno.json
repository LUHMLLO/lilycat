{
  "lock": false,
  "nodeModulesDir": "auto",
  "name": "@luhmllo/lilycat",
  "version": "0.1.30",
  "description": "A minimalist CSS library that keeps HTML clean and styling efficient, embracing the power of native CSS while avoiding utility class bloat.",
  "keywords": [
    "css",
    "ui-kit",
    "design-system",
    "frontend",
    "styling",
    "tokens",
    "components",
    "minimalist",
    "clean-html",
    "utility-css",
    "native-css",
    "opinionated"
  ],
  "homepage": "https://github.com/LUHMLLO/lilycat.css",
  "license": "MIT",
  "exports": "./dist/mod.ts",
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "build": "deno run -A ./build.ts",
    "publish": "deno run -A ./publish.ts"
  },
  "include": [
    "dist/**/*.css",
    "dist/**/*.ts",
    "../LICENSE",
    "../readme.md"
  ],
  "exclude": [
    "./node_modules",
    "./src",
    "./build.ts"
  ]
}