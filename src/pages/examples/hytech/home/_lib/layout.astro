---
import Layout_HTML from 'src/lib/layout.html.astro';

import Banner from './components/banner.astro';
import Appbar from './components/appbar.astro';
import Hero from './components/hero.astro';
---

<Layout_HTML title='Hytech Home'>
	<ly-layer stacked='under'> </ly-layer>
	<ly-app>
		<Banner />
		<Appbar />
		<Hero />
		<slot />
	</ly-app>
	<ly-layer stacked='over'> </ly-layer>
</Layout_HTML>
