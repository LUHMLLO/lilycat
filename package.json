{
	"name": "lilys-ui",
	"description": "Minimalism Meets Modularity for Effortless UI Development",
	"version": "0.0.1",
	"type": "module",
	"main": "./dist/components/index.cjs",
	"module": "./dist/components/index.js",
	"browser": "./dist/components/index.iife.js",
	"author": "luhmllo",
	"license": "MIT",
	"files": [
		"dist/"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/LUHMLLO/m-ui.git"
	},
	"scripts": {
		"stage:1": "sass --no-source-map ./src/lib/design_system/sass/props.theming.scss ./src/lib/design_system/css/props.theming.css",
		"stage:2": "postcss ./src/lib/design_system/css/*.css -d ./dist/css/ --config ./.postcssrc",
		"stage:3": "vite build --config vite.lib.config.ts",
		"stage:4": "vite build --config vite.app.config.ts",
		"build": "pnpm stage:1 && pnpm stage:2 && pnpm stage:3 && pnpm stage:4",
		"check": "tsc",
		"dev": "vite --config vite.app.config.ts",
		"preview": "vite preview --config vite.app.config.ts",
		"publish": "pnpm run build && pnpm version patch && pnpm publish && git push"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.19",
		"cssnano": "^6.1.2",
		"cssnano-preset-advanced": "^6.1.2",
		"eslint-plugin-lit": "^1.11.0",
		"eslint-plugin-wc": "^2.0.4",
		"lit": "^3.0.2",
		"postcss": "^8.4.38",
		"postcss-cli": "^11.0.0",
		"sass": "^1.72.0",
		"terser": "^5.29.2",
		"typescript": "^5.2.2",
		"vite": "^5.2.6"
	},
	"keywords": [
		"agnostic",
		"css",
		"custom elements",
		"library",
		"ui",
		"web components"
	],
	"browserslist": [
		"defaults",
		"last 2 versions",
		"not dead",
		"> 1%"
	]
}
