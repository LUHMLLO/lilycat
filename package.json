{
  "name": "brrr-components",
  "description": "Universal web components library made with Lit Element & Typescript",
  "version": "0.0.3",
  "main": "./src/main.ts",
  "type": "module",
  "files": [
    "dist/index.cjs",
    "dist/index.iife.js",
    "dist/index.js",
    "dist/index.umd.cjs"
  ],
  "scripts": {
    "update:latest": "pnpm update --interactive --latest",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "make:css": "sass ./src/_dev/main.scss:./src/_dev/main.css --no-source-map --charset --style expanded",
    "lib:publish": "tsc && vite build && pnpm version patch && pnpm publish"
  },
  "dependencies": {
    "lit": "^3.0.2"
  },
  "devDependencies": {
    "material-symbols": "^0.14.0",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}
