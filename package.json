{
  "name": "brrr-components",
  "description": "Universal web components library made with Lit Element & Typescript",
  "version": "0.0.6",
  "main": "./src/main.ts",
  "type": "module",
  "files": [
    "dist/index.cjs",
    "dist/index.iife.js",
    "dist/index.js",
    "dist/index.umd.cjs",
    "index.html",
    "readme.md"
  ],
  "scripts": {
    "_css": "sass ./src/_dev/main.scss:./src/_dev/main.css --no-source-map --charset --style expanded",
    "_publish": "tsc && vite build && pnpm version patch && pnpm publish",
    "_update": "pnpm update --interactive --latest",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "lit": "^3.0.2",
    "sass": "^1.69.5",
    "terser": "^5.24.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}
