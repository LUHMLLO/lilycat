{
  "name": "brrr-components",
  "description": "Framework Agnostic web components library made with Lit Element & Typescript",
  "version": "0.0.17",
  "main": "./dist/lib/index.cjs",
  "module": "./dist/lib/index.js",
  "browser": "./dist/lib/index.iife.js",
  "type": "module",
  "files": [
    "dist/*",
    "index.html",
    "readme.md"
  ],
  "scripts": {
    "_build:demo": "tsc && vite build",
    "_build:lib": "tsc && vite build --config vite.lib.config.ts",
    "_publish": "pnpm build && pnpm version patch && pnpm publish && git push",
    "_update": "pnpm update --interactive --latest",
    "dev": "vite",
    "build": "pnpm _build:demo && pnpm _build:lib",
    "preview": "vite preview"
  },
  "devDependencies": {
    "lit": "^3.0.2",
    "prismjs": "^1.29.0",
    "sass": "^1.69.5",
    "terser": "^5.24.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "keywords": [
    "ui",
    "ux",
    "component library",
    "web components",
    "web components api",
    "javascript",
    "js",
    "typescript",
    "ts",
    "html5",
    "html",
    "lit-html",
    "lit-element",
    "litElement",
    "Brrr",
    "Framework Agnostic"
  ]
}